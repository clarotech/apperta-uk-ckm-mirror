definition
	CLUSTER[at0000] matches {    -- test
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {*}    -- test identifier
			ELEMENT[at0003] occurrences matches {1..*} matches {    -- boohoo
				value matches {
					DV_PROPORTION matches {*}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- DV_IDENTIFIER
				value matches {
					DV_IDENTIFIER matches {
						issuer matches {/DEF-234/}
						type matches {/ERF-232/}
						assigner matches {/XYZ-234/}
						id matches {/ABC-123/}
					}
				}
				name matches {
					DV_CODED_TEXT matches {*}
				}
			}
			ELEMENT[at0005] occurrences matches {0..1} matches {    -- parse
				value matches {
					DV_PARSABLE matches {
						formalism matches {"text/xml","text/rtf","aaa/ccc"}
					}
				}
			}
			ELEMENT[at0006] occurrences matches {0..1} matches {    -- MyQuantity
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::124]>
						list = <
							["1"] = <
								units = <"dg">
								magnitude = <|2.0..12.0|>
								precision = <|1|>
								units_system = <"http://unitsofmeasure.org">
								units_display_name = <"dg">
							>
							["2"] = <
								units = <"g">
								magnitude = <|>122.0..<1123.0|>
								precision = <|2|>
								units_system = <"http://unitsofmeasure.org">
								units_display_name = <"g">
							>
						>

						assumed_value = <
							units = <"dg">
							magnitude = <6.0>
							precision = <1>
						>
					>
				}
			}
		}
	}

